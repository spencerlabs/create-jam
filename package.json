{
  "name": "create-jam",
  "description": "Command line interface for quickly spinning up new Jamstack apps",
  "version": "1.1.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/spencerlabs/create-jam"
  },
  "homepage": "https://createjam.app",
  "bugs": {
    "url": "https://github.com/spencerlabs/create-jam/issues"
  },
  "keywords": [
    "jam",
    "create jam",
    "jamstack",
    "create jamstack app",
    "astro",
    "blitz",
    "docusaurus",
    "eleventy",
    "gatsby",
    "hexo",
    "nest",
    "next js",
    "nuxt",
    "preact",
    "react",
    "redux",
    "redwood",
    "remix",
    "vite"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/spencerlabs"
  },
  "bin": "./dist/index.js",
  "license": "MIT",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "cmd": "ts-node src/index.ts example-app",
    "cmd:built": "node dist/index.js example-build-app",
    "docs:serve": "npx docsify serve ./docs",
    "test": "jest",
    "build": "rm -rf dist && tsc -p . && mv ./dist/src/* ./dist/"
  },
  "dependencies": {
    "create-astro": "latest",
    "create-docusaurus": "latest",
    "create-next-app": "latest",
    "create-nuxt-app": "latest",
    "create-react-app": "latest",
    "create-redwood-app": "latest",
    "create-remix": "latest",
    "create-vite": "latest",
    "execa": "5.1.1",
    "prompts": "^2.4.2",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@spencerlabs/eslint-config": "^1.0.0",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.17",
    "@types/prompts": "^2.4.4",
    "jest": "^29.3.1",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "eslintConfig": {
    "extends": "@spencerlabs/eslint-config",
    "root": true,
    "parserOptions": {
      "sourceType": "module"
    },
    "ignorePatterns": [
      "templates/**/*",
      "dist/**/*"
    ]
  },
  "nodemonConfig": {
    "watch": "./src",
    "exec": "bin/create-jam",
    "ext": "ts"
  },
  "packageManager": "yarn@3.3.1"
}
