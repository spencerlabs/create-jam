    "eslint-plugin-prettier": "^4.2.1",
    "execa": "7.1.1",
    "fs-extra": "^11.1.1",
    "jest": "^29.5.0",
    "kolorist": "^1.8.0",
    "minimist": "^1.2.8",
    "prettier": "^3.0.0",
