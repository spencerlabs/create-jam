{
  "name": "create-jam",
  "description": "Command line interface for quickly spinning up new Jamstack apps",
  "version": "2.0.0-rc.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/spencerlabs/create-jam"
  },
  "homepage": "https://createjam.app",
  "bugs": {
    "url": "https://github.com/spencerlabs/create-jam/issues"
  },
  "keywords": [
    "jam",
    "create jam",
    "jamstack",
    "create jamstack app",
    "astro",
    "blitz",
    "docusaurus",
    "eleventy",
    "gatsby",
    "hexo",
    "nest",
    "next js",
    "nuxt",
    "preact",
    "react",
    "redux",
    "redwood",
    "remix",
    "vite"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/spencerlabs"
  },
  "bin": {
    "create-jam": "index.js",
    "create-jam-app": "index.js"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "docs:serve": "npx docsify serve ./docs"
  },
  "dependencies": {
    "create-astro": "latest",
    "create-docusaurus": "latest",
    "create-next-app": "latest",
    "create-nuxt-app": "latest",
    "create-react-app": "latest",
    "create-redwood-app": "latest",
    "create-remix": "latest",
    "create-vite": "latest"
  },
  "devDependencies": {
    "@spencerlabs/eslint-config": "^1.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.17",
    "@types/prompts": "^2.4.4",
    "execa": "5.1.1",
    "kolorist": "^1.7.0",
    "minimist": "^1.2.8",
    "prompts": "^2.4.2",
    "typescript": "^5.0.2",
    "unbuild": "^1.2.0"
  },
  "eslintConfig": {
    "extends": "@spencerlabs/eslint-config",
    "root": true,
    "parserOptions": {
      "sourceType": "module"
    },
    "ignorePatterns": [
      "templates/**/*",
      "dist/**/*"
    ]
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "packageManager": "yarn@3.3.1"
}
